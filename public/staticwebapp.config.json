{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/api/login",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/register",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous", "authenticated"]
    }
  ],
  "globalHeaders": {
    "Cache-Control": "no-store, no-cache, must-revalidate, proxy-revalidate"
  }
}